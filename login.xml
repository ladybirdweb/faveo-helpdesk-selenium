        <classes>
            <class name="regression.LoginTest">
                <methods>
                    <parameter name="userName" value="invalidusername"/>
                    <parameter name="password" value="test123!"/>
                    <include name="login"/>
                    <include name="invalidLoginDetailsCheck"/>
                </methods>
            </class>
        </classes>
    </test>

    <test name="Admin_ValidUsername_InvalidPassword">

        <classes>
            <class name="regression.LoginTest">
                <methods>
                    <parameter name="userName" value="testadmin"/>
                    <parameter name="password" value="invalidpassword"/>
                    <include name="login"/>
                    <include name="invalidLoginDetailsCheck"/>
                </methods>
