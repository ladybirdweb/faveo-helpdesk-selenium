<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="LOGIN">
    <listeners>
        <listener class-name="commontests.Listeners"/>
    </listeners>

    <test name="Admin_ValidUsername_ValidPassword">

        <classes>
            <class name="regression.LoginTest">
                <methods>
                    <parameter name="userName" value="testadmin"/>
                    <parameter name="password" value="testpass"/>
                    <include name="login"/>
                    <include name="adminLoginCheck"/>
                </methods>
            </class>
        </classes>
    </test>

    <test name="Admin_ValidEmail_ValidPassword">

        <classes>
            <class name="regression.LoginTest">
                <methods>
                    <parameter name="userName" value="kvb422@gmail.com"/>
                    <parameter name="password" value="testpass"/>
                    <include name="login"/>
                    <include name="adminLoginCheck"/>
                </methods>
            </class>
        </classes>
    </test>


    <test name="Agent_ValidUsername_ValidPassword">

        <classes>
            <class name="regression.LoginTest">
                <methods>
                    <parameter name="userName" value="a1"/>
                    <parameter name="password" value="test123!"/>
                    <include name="login"/>
                    <include name="agentLoginCheck"/>
                </methods>
            </class>
        </classes>
    </test>

    <test name="OrganizationManager_ValidUsername_ValidPassword">

        <classes>
            <class name="regression.LoginTest">
                <methods>
                    <parameter name="userName" value="vk"/>
                    <parameter name="password" value="test123!"/>
                    <include name="login"/>
                    <include name="OrganizationManagerUserLoginCheck"/>
                </methods>
            </class>
        </classes>
    </test>

