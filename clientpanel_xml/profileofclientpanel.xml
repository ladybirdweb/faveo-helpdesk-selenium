<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="PROFILETESTSOFCLIENTPANEL">
    <listeners>
        <listener class-name="commontests.Listeners"/>
    </listeners>


    <test name="profilePageOfClientCapture">

        <classes>
            <class name="clientpaneltests.ChangePasswordOfClientTest">
                <parameter name="userName" value="vk"/>
                <parameter name="password" value="test123!"/>
                <methods>
                    <include name="profilePageOfClientCapture"/>
                </methods>
            </class>
        </classes>
    </test>

    <test name="checkEmailFieldInProfileOfClientPanel">

        <classes>
            <class name="clientpaneltests.ChangePasswordOfClientTest">
                <parameter name="userName" value="veera.kada@ladybirdweb.com"/>
                <parameter name="password" value="test123!"/>

                <methods>
                    <include name="checkEmailFieldInProfileOfClientPanel"/>
                </methods>
            </class>
        </classes>
    </test>

</suite>